#include<iostream>
using namespace std;
int main()
{
    int a[20],n,i,j,r;
    cout<<"enter the number of slices"<<endl;
    cin>>n;
    cout<<"enter the tastiness level"<<endl;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    i=0;
    j=n-1;
    while(i<n-1)
    {
        if(i==j)
        {
          r=a[i];
          break;
        }
        if(a[i]>a[j])
        {
            i++;
        }
        else{
            j--;
        } 
    }
    cout<<r<<endl;
}